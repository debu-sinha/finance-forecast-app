{
  "name": "databricks-forecast-for-finance",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "author": "Debu Sinha",
  "description": "Finance Forecasting Platform for Databricks",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "backend": "cd backend && uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "start": "npm run build",
    "start:local": "concurrently \"npm run backend\" \"npm run dev\"",
    "clean": "rm -rf dist node_modules backend/__pycache__"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.1.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}